<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!-- Modal -->
<div class="modal fade" id="modal" role="dialog" style="z-index: 999">
  <div class="modal-dialog mt100">
    <div class="modal-content">
      <div class="modal-header" style="background-color: #807097; color: white; font-weight: bold; ">
        <button type="button" class="close" data-dismiss="modal"  style="color: white; opacity: 100%;">×</button>
        <h4 id="modalTitle"  class="modal-title" style="text-align: center;"> </h4>
      </div>
      <div class="modal-body">
        <p style="text-align: center; font-weight: bold; color:#333030 " id="modalContent"></p>
      </div>
      <div class="modal-footer">
		<div class="form-inline " role="group">
        	<a href="javascript:void(0);" type="button" class="pop-btn-full" data-dismiss="modal">닫기</a>
       	</div>
      </div>
    </div>
    
  </div>
</div>
